    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "77aba1f5-de8e-4a41-97eb-cefb09381416"],
 ", con ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["p", "TI"], 
         RowBox[{"(", 
           StyleBox["x", "TI"], ")"}], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["x", "TI"], "4"], "+", 
         StyleBox["x", "TI"], "+", "1"}], TraditionalForm], "errors" -> {}, 
